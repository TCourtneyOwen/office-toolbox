{
  "name": "office-toolbox",
  "version": "0.1.1",
  "description": "Tools for Office Add-ins",
  "main": "./app/util.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf app && concurrently \"npm run tsc\"",
    "start": "rimraf app && concurrently \"npm run tsc-watch\"",
    "run": "office-toolbox"
  },
  "keywords": [
    "office"
  ],
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-toolbox": "./app/office-toolbox.js"
  },
  "files": [
    "app",
    "templates"
  ],
  "dependencies": {
    "applicationinsights": "^1.0.3",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "fs-extra": "^3.0.1",
    "inquirer": "^6.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jszip": "^3.1.3",
    "junk": "^2.1.0",
    "mime": "^2.4.4",
    "node-powershell": "^3.3.1",
    "office-addin-validator": "^1.0.5",
    "opn": "^5.3.0",
    "request": "^2.88.0",
    "xml2js": "^0.4.17",
    "xmlhttprequest": "1.8.0",
    "onedrive-api": "0.3.1"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-client": "^1.7.0",
    "@types/applicationinsights": "^0.20.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jszip": "^3.1.4",
    "@types/mime": "^2.0.1",
    "@types/node-powershell": "^3.1.0",
    "@types/request": "^2.48.1",
    "@types/xml2js": "^0.4.3",
    "concurrently": "^3.5.0",
    "rimraf": "^2.6.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "msal": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficeDev/office-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/office-toolbox/issues"
  },
  "homepage": "https://github.com/OfficeDev/office-toolbox#readme"
}
